{
  "name": "token-place",
  "version": "1.0.0",
  "private": true,
  "description": "p2p generative AI platform",
  "main": "index.js",
  "scripts": {
    "test": "pytest tests/",
    "test:js": "node tests/test_js_crypto.js && node tests/test_js_mock_server.js && node tests/test_chat_typing_effect.js && npx ts-node --project tsconfig.json tests/test_token_place_client_package.ts && npx ts-node --project tsconfig.json tests/test_token_place_client.ts && npx ts-node --project tsconfig.json tests/test_integration_setup.ts",
    "test:crypto-compat": "python tests/test_crypto_compatibility.py",
    "test:e2e": "pytest tests/test_e2e_chat.py",
    "test:all": "npm run test && npm run test:js && npm run test:crypto-compat && npm run test:e2e",
    "lint": "eslint --max-warnings 0 \"clients/**/*.ts\" \"tests/**/*.ts\"",
    "test:ci": "./run_all_tests.sh",
    "type-check": "echo \"No type-check configured\"",
    "build": "echo \"No build configured\"",
    "build:client": "tsc --project tsconfig.client.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futuroptimist/token.place.git"
  },
  "keywords": [
    "AI",
    "encryption",
    "LLM",
    "generative",
    "P2P"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "bugs": {
    "url": "https://github.com/futuroptimist/token.place/issues"
  },
  "homepage": "https://github.com/futuroptimist/token.place#readme",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "jsencrypt": "^3.5.4",
    "mocha": "^11.7.4",
    "openai": "^3.2.1",
    "selenium-webdriver": "^4.35.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.13.0",
    "globals": "^15.14.0",
    "playwright": "^1.56.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
