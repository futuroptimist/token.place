{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": true,
    "outDir": "./clients/dist",
    "rootDir": "./clients",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["ES2020", "DOM"],
    "types": ["node"],
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "include": ["clients/index.ts", "clients/token_place_client.ts"]
}
